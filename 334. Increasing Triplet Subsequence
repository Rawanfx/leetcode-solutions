public class Solution {
    public bool IncreasingTriplet(int[] arr) {
      long x1=1000000000;
      long x2 =1000000000;
      for (int i=0;i<arr.Length;i++){
        if (arr[i]<=x1){
            x1=arr[i];
        }
        else if (arr[i]<=x2){
            x2=arr[i];
        }
        if (arr[i]>x1 && arr[i]>x2 && x1!=x2)
        return true;
      }
      return false;
    }
}
